<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="cn.darkflame.meta.i18n.I18NInfo">
        <table name="i18n_info" schema="public" catalog="darkflame"/>
        <id-class class="cn.darkflame.meta.i18n.I18NInfoPK"/>
        <attributes>
            <id name="resourceFile">
                <column name="resource_file"/>
            </id>
            <id name="code">
                <column name="code"/>
            </id>
            <id name="number">
                <column name="number"/>
            </id>
            <basic name="info">
                <column name="info"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
